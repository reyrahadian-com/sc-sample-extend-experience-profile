		<model>
			<elements>				
				<element interface="Sitecore.TC.ExperienceProfile.ContactFacets.ICustomFieldsFacet, Sitecore.TC.ExperienceProfile"
				         implementation="Sitecore.TC.ExperienceProfile.ContactFacets.CustomFieldsFacet, Sitecore.TC.ExperienceProfile" />
